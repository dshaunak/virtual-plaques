<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:mapbox="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Map">
    <!--This is a valid comment.
     Above, I have replaced:
     xmlns:app="http://schemas.android.com/apk/res-auto"
     with,
     xmlns:mapbox="http://schemas.android.com/apk/res-auto"

     Below, I have used 'mapbox:mapbox_cameraTargetLat.....'
     instead of,
     'app:mapbox_cameraTargetLat.....'

     ''''''''Cut+Pasted from MapView to remove error
     mapbox:mapbox_styleUrl="mapbox://styles/mapbox/streets-v10"
     -->
    <com.mapbox.mapboxsdk.maps.MapView
        android:id="@+id/mapV"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        mapbox:layout_constraintBottom_toBottomOf="parent"
        mapbox:layout_constraintEnd_toEndOf="parent"
        mapbox:layout_constraintStart_toStartOf="parent"
        mapbox:layout_constraintTop_toTopOf="parent"
        mapbox:mapbox_cameraTargetLat="37.7749"
        mapbox:mapbox_cameraTargetLng="-122.4194"
         />

</androidx.constraintlayout.widget.ConstraintLayout>